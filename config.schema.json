{
  "pluginAlias": "SmartSprinklers",
  "headerDisplay": "Homebridge plugin for climate based adaptive irrigation sprinkler control. Default [values][range] mentioned in description.",
  "footerDisplay": "Please visit https://github.com/MTry/homebridge-smart-irrigation for more details.<br> Created by Mayank Tandon - @MTry",
  "pluginType": "accessory",
  "singular": true,
 
  "schema": {
    "type": "object",
    "properties": {
      "accessory": {
          "default": "SmartSprinklers",
          "type": "string"
      },
      "name": {
          "title": "Accessory Name:",
          "description": "Name for the accessory [Irrigation]",
          "type": "string",
          "required": true,
          "default": "Irrigation"
        },
      "verbosed": {
      "title": "Verbose Climate data",
      "description": "Enable for more detailed display. [true]",
      "type": "boolean",
      "required": false,
      "default": true
      },
      "masterDisable": {
      "title": "Disable scheduling",
      "description": "Disable scheduling irrigation. [false]",
      "type": "boolean",
      "required": false,
      "default": false
      },
      "exposeControls": {
      "title": "Expose Controls",
      "description": "Expose Controls to Home App. [true]",
      "type": "boolean",
      "required": false,
      "default": true
      },
      "recheckTime": {
      "title": "Recheck Time",
      "description": "Reassess - minutes before runtime.[None]",
      "type": "number",
      "default": 0,
      "enum": [15, 30, 60 ]
      },
      "latitude": {
      "title": "Latitude:",
      "description": "Enter the latitude in decimals including '-' if in the southern hemisphere. [-90 - 90]",
      "type": "number",
      "required": true,
      "minimum": -90,
      "maximum": 90,
      "default": 0.0
      },
      "longitude": {
      "title": "Longitude:",
      "description": "Enter the longitude in decimals. [-180 - 180]",
      "type": "number",
      "required": true,
      "minimum": -180,
      "maximum": 180,
      "default": 0.0
      },
      "altitude": {
      "title": "Altitude:",
      "description": "Enter the altitude in meters. [0-10000m]",
      "type": "number",
      "required": true,
      "minimum": 0,
      "maximum": 10000,
      "default": 0.0
      },
      "keyAPI": {
      "title": "API Key:",
      "description": "Enter your OpenWeatherMap API Key.",
      "type": "string",
      "placeholder": "Your OWM Key",
      "required": true
      },
      "cycles": {
      "title": "Cycles per zone:",
      "description": "Number of cycles per zone (watering is spread between cycles). [2][1 - 5]",
      "type": "number",
      "required": false,
      "minimum": 1,
      "maximum": 5,
      "default": 2
      },
      "sunriseOffset": {
      "title": "Sunrise Offest:",
      "description": "Minutes before the sunrise watering should get over by. [0]",
      "type": "number",
      "required": false,
      "minimum": -1440,
      "maximum": 1440,
      "default": 0
      },        
     "lowThreshold": {
      "title": "Low Threshold Temperature:",
      "description": "Skip scheduling when forecasted minimum temperature falls below this. [5Â°C][0Â°C - 20Â°C]",
      "type": "number",
      "required": false,
      "minimum": 0,
      "maximum": 20,
      "default": 5
      },
      "highThreshold": {
      "title": "High Threshold Temperature:",
      "description": "Skip scheduling when forecasted maximum temperature stays below this. [10Â°C][5Â°C - 30Â°C]",
      "type": "number",
      "required": false,
      "minimum": 5,
      "maximum": 30,
      "default": 10
      },
      "emailEnable": {
        "title": "Enable",
        "description": "Enable Notifications",
        "type": "boolean",
        "required": false,
        "default": false
        },
        "senderName": {
        "title": "Sender Name:",
        "type": "string",
        "required": true,
        "default": "ðŸ’¦ Irrigation Control"
        },      
        "senderEmail": {
        "title": "Sender Email ID:",
        "type": "string",
        "pattern": "^\\S+@\\S+$",
        "required": true,
        "placeholder": "abc@xyz.com"
        },
        "sendTo": {
        "title": "Receiver Email ID:",
        "type": "string",
        "pattern": "^\\S+@\\S+$",
        "required": true,
        "placeholder": "abc@xyz.com"
        },
        "smtpHost": {
        "title": "SMTP Host:",
        "type": "string",
        "required": true,
        "placeholder": "hostname"
        },
        "smtpPort": {
        "title": "SMTP Port:",
        "type": "number",
        "required": true,
        "default": 587
        },
        "portSecure": {
        "title": "Secure Port",
        "type": "boolean",
        "required": false,
        "default": false
        },      
        "userID": {
        "title": "SMTP Username:",
        "type": "string",
        "required": true,
        "placeholder": "username"
        },
        "userPwd": {
        "title": "SMTP Password:",
        "type": "string",
        "required": true,
        "placeholder": "password"
        },
      "pushEnable": {
        "title": "Enable",
        "description": "Enable Push Notifications",
        "type": "boolean",
        "required": false,
        "default": false
        },
        "userPO": {
        "title": "User Key:",
        "type": "string",
        "required": true,
        "placeholder": "User key"
        },      
        "tokenPO": {
        "title": "Application Token:",
        "type": "string",
        "required": true,
        "placeholder": "App token"
        },
        "devicePO": {
        "title": "Receiving Device(s):",
        "type": "string",
        "placeholder": "devices"
        },
      "priorityPO": {
        "title": "Message Priority:",
        "type": "integer",
        "default": 0,
        "required": false,
        "oneOf": [
            {
              "title": "Lowest (-2)",
              "enum": [
                -2
              ]
            },
            {
              "title": "Low (-1)",
              "enum": [
                -1
              ]
            },
            {
              "title": "Normal (0)",
              "enum": [
                0
              ]
            },
            {
              "title": "High (1)",
              "enum": [
                1
              ]
            },
            {
              "title": "Emergency (2)",
              "enum": [
                2
              ]
            }
          ]
        },
        "soundPO": {
        "title": "Notification Sound:",
        "type": "string",
        "default": "pushover",
        "oneOf": [
            {
              "title": "pushover",
              "enum": [
                "pushover"
