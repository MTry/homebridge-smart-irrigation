              "enum": [
                "mechanical"
              ]
            },
            {
              "title": "pianobar",
              "enum": [
                "pianobar"
              ]
            },
            {
              "title": "siren",
              "enum": [
                "siren"
              ]
            },
            {
              "title": "spacealarm",
              "enum": [
                "spacealarm"
              ]
            },
            {
              "title": "tugboat",
              "enum": [
                "tugboat"
              ]
            },
            {
              "title": "alien",
              "enum": [
                "alien"
              ]
            },
            {
              "title": "climb",
              "enum": [
                "climb"
              ]
            },
            {
              "title": "persistent",
